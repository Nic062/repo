<package>
	<id>com.level3tjg.bfdecrypt_1.3.1</id>
	<name>BFDecrypt</name>
	<version>1.3.5</version>
	<compatibility>
		<firmware>
			<miniOS>11.0</miniOS>
			<otherVersions>unsupported</otherVersions>
			<!--
			for otherVersions, you can put either unsupported or unconfirmed
			-->
		</firmware>
	</compatibility>
	<dependencies>
		<package>PreferenceLoader</package>
		<package>applist</package>
	</dependencies>
